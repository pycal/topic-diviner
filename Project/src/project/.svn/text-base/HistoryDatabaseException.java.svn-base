package project;

import java.io.IOException;

/**
 * Exception raised when HistoryDatabaseManager does not work as it should.
 */
public class HistoryDatabaseException extends IOException {

    /**
     * Creates an Exception and records a custom message as the exception.
     * @param message A user friendly description of what went wrong.
     */
    public HistoryDatabaseException(String message) {
        super(message);
    }
}
